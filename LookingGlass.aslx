  <function name="moveKnight">
    if (game.pov.parent = Bishop Entrance) {
      RemoveObject (Knight)
    }
    else if (Knight.position = "C3") {
      MoveObject (Knight, B1)
      Knight.position = "B1"
      C3.inRoom = "#"
    }
    else if (Knight.position = "B1") {
      MoveObject (Knight, A3)
      Knight.position = "A3"
      B1.inRoom = "#"
    }
    else if (Knight.position = "A3") {
      MoveObject (Knight, B5)
      Knight.position = "B5"
      A3.inRoom = "#"
    }
    else if (Knight.position = "B5") {
      MoveObject (Knight, E4)
      Knight.position = "E4"
      B5.inRoom = "#"
    }
    else if (Knight.position = "E4") {
      MoveObject (Knight, F2)
      Knight.position = "F2"
      E4.inRoom = "#"
    }
    else if (Knight.position = "F2") {
      MoveObject (Knight, D1)
      Knight.position = "D1"
      F2.inRoom = "#"
    }
    else if (Knight.position = "D1") {
      MoveObject (Knight, B2)
      Knight.position = "B2"
      D1.inRoom = "#"
    }
    else if (Knight.position = "B2") {
      MoveObject (Knight, A4)
      Knight.position = "A4"
      B2.inRoom = "#"
    }
    else if (Knight.position = "A4") {
      MoveObject (Knight, C3)
      Knight.position = "C3"
      A4.inRoom = "#"
    }
    else if (Knight.position = "A2") {
      MoveObject (Knight, C3)
      Knight.position = "C3"
      A2.inRoom = "#"
    }
    else {
      msg ("ERROR")
      finish
    }
    if (player.parent = Knight.parent) {
      deathByKnight
    }
  </function>
